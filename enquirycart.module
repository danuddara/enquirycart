/**
 * Implements hook_mail() for enquirycartform submit.
 *
 * This is created for the mail used in formsubmit.
 */
function enquirycart_mail($key, &$message, $params) {

  switch ($key) {
    case 'Send_enquiry':
      $message['subject'] = $params['subject'];
      $message['body'][] = $params['message'];
      break;

  }
}
